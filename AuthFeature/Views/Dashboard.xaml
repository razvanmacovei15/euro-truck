<Page
    x:Class="EuroTruck.AuthFeature.Views.DashboardPage"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:EuroTruck.AuthFeature.Views">

    <StackPanel HorizontalAlignment="Center" VerticalAlignment="Center" Spacing="10">
        <TextBlock Text="Welcome to the Dashboard!" FontSize="24" FontWeight="Bold" />
        <TextBlock Text="You have successfully logged in." />
        <Button Content="Log out" Click="OnLogoutClicked" />
    </StackPanel>
</Page>
